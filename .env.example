# WalletConnect Cloud project identifier
VITE_WALLET_CONNECT_PROJECT_ID=your_walletconnect_project_id

# Trustware SDK API key for the bridge widget
VITE_TRUSTWARE_API_KEY=your_trustware_api_key

# Alchemy Base RPC endpoint used for on-chain stats (e.g., https://base-mainnet.g.alchemy.com/v2/your-key)
VITE_ALCHEMY_BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/demo
# Alchemy Ethereum RPC endpoint used for Aave contract reads (e.g., https://eth-mainnet.g.alchemy.com/v2/your-key)
VITE_ALCHEMY_ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/demo
